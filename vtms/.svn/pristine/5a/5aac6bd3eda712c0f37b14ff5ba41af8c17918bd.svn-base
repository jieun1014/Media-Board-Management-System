<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.transfer.video.vtms.common.AccountMapper">
	<select id="select" parameterType="com.transfer.video.vtms.common.Account" resultType="com.transfer.video.vtms.common.Account">
		SELECT
		     ID
		    ,PASSWORD
		FROM ACCOUNT
		WHERE ID=#{id}
	</select>
</mapper>